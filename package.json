{
  "name": "@antmjs/ant-temp",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "main": "index.js",
  "scripts": {
    "eslint": "npx eslint -c eslint.config.js '**/*.{js,jsx,ts,tsx}' && npx tsc -p tsconfig.json --skipLibCheck",
    "stylelint": "npx stylelint --aei --config stylelint.config.js '**/*.{css,less}'",
    "prettier": "npx prettier --write '**/*.{js,jsx,ts,tsx,md,html,css,less}'",
    "prepare": "npx husky install",
    "start": "yarn dev",
    "dev": "vite",
    "build": "vite build",
    "mock": "antm-api watch --path ./src/actions/types --mock true  --action true",
    "page-server": "npx ts-node ./src/components/page-insert/server.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@antmjs/api": "^2.1.1",
    "@antmjs/cache": "^2.1.1",
    "@antmjs/trace": "^2.1.1",
    "@antmjs/warning": "^2.0.7",
    "@babel/generator": "^7.20.3",
    "@babel/parser": "^7.20.3",
    "@babel/template": "^7.18.10",
    "@babel/traverse": "^7.20.1",
    "@babel/types": "^7.20.2",
    "antd": "^4.4.3",
    "axios": "^1.0.0",
    "dayjs": "^1.11.5",
    "deepmerge": "^4.2.2",
    "eslint": "8.21.0",
    "node-dir-tree": "^0.2.0",
    "node-ts": "^5.1.2",
    "path-to-regexp": "^6.2.1",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "recoil": "^0.7.5",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@antmjs/eslint": "^2.1.0",
    "@antmjs/stylelint": "^2.1.0",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-legacy": "^2.2.0",
    "@vitejs/plugin-react": "^2.1.0",
    "chokidar": "^3.5.3",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "glob": "^8.0.3",
    "globby": "^13.1.2",
    "husky": "^7.0.1",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "lint-staged": "^11.0.1",
    "os": "^0.1.2",
    "prettier": "^2.7.1",
    "stylelint": "^14.11.0",
    "typescript": "^4.5.4",
    "vite": "^3.1.3",
    "vite-plugin-html": "^3.2.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "type-enum": [
        2,
        "always",
        [
          "feat",
          "fix",
          "style",
          "chore",
          "typings",
          "docs",
          "refactor",
          "test"
        ]
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog",
      "types": {
        "feat": {
          "description": "新功能（feature）"
        },
        "fix": {
          "description": "修补bug"
        },
        "style": {
          "description": "格式（不影响代码运行的变动）"
        },
        "chore": {
          "description": "构建过程或辅助工具的变动"
        },
        "typings": {
          "description": "Typescript 类型错误"
        },
        "docs": {
          "description": "文档（documentation）"
        },
        "refactor": {
          "description": "重构（既不是新增功能，也不是修改bug的代码变动）"
        },
        "test": {
          "description": "增加或修改测试用例"
        }
      }
    }
  },
  "engines": {
    "node": ">=16",
    "npm": ">=6.4",
    "yarn": ">=1.22"
  }
}
